{
	"manifest_version": 3,
	"name": "Vim Navigation",
	"version": "1.0",
	"description": "Simple navigation using vim keys",
	"icons": {
	"16": "./assets/icon-16.png",
	"32": "./assets/icon-32.png",
    "48": "./assets/icon-48.png",
	"128": "./assets/icon-128.png"
	},
	"action": {
		"default_icon": {
			"48": "./assets/icon-48.png"
		}
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["main.js"]
		}
	]
}
